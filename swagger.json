    "/api/electricity/current": {
      "post": {
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "required": true,
            "schema": {
              "properties": {
                "resistance": {
                  "description": "Resistance (Ω)",
                  "example": 4,
                  "type": "number"
                },
                "voltage": {
                  "description": "Voltage (V)",
                  "example": 12,
                  "type": "number"
                }
              },
              "required": ["voltage", "resistance"],
              "type": "object"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful calculation",
            "schema": {
              "properties": {
                "result": {
                  "description": "Current (Amperes)",
                  "type": "number"
                }
              },
              "type": "object"
            }
          },
          "400": {
            "description": "Invalid input or Zero Division"
          }
        },
        "summary": "Calculate Current (I)",
        "tags": ["Electricity"]
      }
    },
